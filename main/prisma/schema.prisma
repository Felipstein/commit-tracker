generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Commit {
  id          String   @id @default(cuid())
  hash        String   @unique
  message     String
  authorName  String
  authorEmail String
  isSubmitted Boolean  @default(false)
  committedAt DateTime @default(now())
  redirectUrl String

  @@map("commits")
}
